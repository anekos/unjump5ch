{
  "description": "Remove the jump from 5ch.net. Just install this addon.",
  "manifest_version": 2,
  "name": "unjump5ch",
  "version": "1.0.13",
  "icons": {
    "64": "icon/j-black.svg"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.5ch.net/*"
      ],
      "js": [
        "js/content.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "applications": {
    "gecko": {
      "id": "unjump5ch@snca.net",
      "strict_min_version": "57.0"
    }
  }
}
